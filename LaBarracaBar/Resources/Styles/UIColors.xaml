<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <Color x:Key="primaryBackColor1">#1C1C1C</Color>
    <Color x:Key="primaryBackColor2">#000000</Color>
    <Color x:Key="secondaryBackColor1">#7A3D3D</Color>
    <Color x:Key="secondaryBackColor2">#B71C1C</Color>

    <!--Window Border Colors-->
    <Color x:Key="winBorderColor1">#013301</Color>
    <Color x:Key="winBorderColor2">#368036</Color>
    <Color x:Key="winBorderColor3">#114C11</Color>
    <Color x:Key="winBorderColor4">#ff9944</Color>
    <Color x:Key="winBorderColor5">DarkOrange</Color>
    <Color x:Key="winBorderColor6">#ff4f00</Color>

    <!--Color Palette-->
    <SolidColorBrush x:Key="color1" Color="DarkGray"/>
    <SolidColorBrush x:Key="color2" Color="DarkRed"/>
    <SolidColorBrush x:Key="color3" Color="Red"/>
    <SolidColorBrush x:Key="color4" Color="#333833"/>
    <SolidColorBrush x:Key="color5" Color="#4C6B4A"/>
    <SolidColorBrush x:Key="color6" Color="#1B6653"/>
    <SolidColorBrush x:Key="color7" Color="#469984"/>
    <SolidColorBrush x:Key="color8" Color="#87CCBB"/>
    <SolidColorBrush x:Key="color9" Color="#033327"/>
    <SolidColorBrush x:Key="color10" Color="#22a"/>
    <SolidColorBrush x:Key="color11" Color="Black"/>
    <SolidColorBrush x:Key="color12" Color="DarkOrange"/>
    <SolidColorBrush x:Key="color13" Color="#ff4f00"/>

    <!--Font Colors-->
    <SolidColorBrush x:Key="tittleColor1" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="tittleColor2" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="tittleColor3" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#E0E1F1"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#E0E1F1"/>

    <!--Panel Colors-->
    <Color x:Key="panelColor1">#FF3A3A3A</Color>
    <Color x:Key="panelColor2">#FF3D3D3C</Color>
    <Color x:Key="panelColor3">#DE3540</Color>
    <SolidColorBrush x:Key="panelOverColor1" Color="#284529"/>
    <SolidColorBrush x:Key="panelOverColor2" Color="#539E4D"/>
    <SolidColorBrush x:Key="panelOverColor3" Color="#cf3a24"/>
    <SolidColorBrush x:Key="panelOverColor4" Color="#f87858"/>
    <SolidColorBrush x:Key="panelOverColor5" Color="DarkOrange"/>
    <SolidColorBrush x:Key="panelOverColor6" Color="#ff4f00"/>

    <!--Button Colors-->
    <SolidColorBrush x:Key="ButtonBackColor1" Color="Gray"/>
    <SolidColorBrush x:Key="ButtonBackColor2" Color="#FF3A3A3A"/>
    <SolidColorBrush x:Key="ButtonBackColor3" Color="#368036"/>
    <SolidColorBrush x:Key="ButtonBackColor4" Color="#22661E"/>
    <SolidColorBrush x:Key="ButtonBackColor5" Color="DarkOrange"/>
    <SolidColorBrush x:Key="ButtonBackColor6" Color="#ff4f00"/>
    <SolidColorBrush x:Key="ButtonTextColor1" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ButtonTextColor2" Color="Gray"/>
    <SolidColorBrush x:Key="ButtonTextColor3" Color="DarkOrange"/>

    <!--TextBox Colors-->
    <SolidColorBrush x:Key="TextBoxColor1" Color="DarkGray"/>
    <SolidColorBrush x:Key="TextBoxColor2" Color="#FFFFFF"/>

</ResourceDictionary>